<div id="headerFooter">
<link rel="stylesheet" href="css/headerFooter.css">
<header id="header">
    <p>
        <img id = "logo" src="tempAssets\globe.png" alt="SAC Logo">
        <h1 id="title">SAC Website</h1>
        <button id="settings" type="button">Accessibility Settings</button>
        <% if (loggedIn) { %>
            <button id="settings" type="button"><a href="/profile">Profile</a></button>
        <% } %>

        <input id = "searchText" type="text" placeholder="Search..">
        <button id = "searchButt" type="submit"><img id = "searchImg" src="tempAssets\search.png" alt="Search"></button>

        <nav>
            <ul id="navigate">
                <li id = "navButt"><a id = "homeButt" href = '/'>Home</a></li>
                <li id = "navButt"><a href = /about-us>About Us</a></li>
                <li id = "navButt">FAQ</li>
                <li id = "navButt">Our Goals</li>
                <% if (!loggedIn) { %>
                    <li id = "navButt"><a href="/sign-up">Sign Up</a></li>
                <% } else { %>
                    <li id = "navButt"><a href="/logout">Logout</a></li>
                <% } %>

                <li id="secret">secretttttttttttttt</li>
            </ul>
        </nav>
    </p>
</header>
</div>
